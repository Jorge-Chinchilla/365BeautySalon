<div class="container p-4">
    <div class="row">

        {{#each productos}}
            <lu>
                <label class="m-1" > Producto ID: {{id}}</label><label class="m-1">Nombre: {{nombre}}</label>
                <label class="m-1">Cantidad: {{cantidad}}</label><label class="m-1">Precio Compra: {{precio_compra}}</label>
                <label class="m-1">Precio Venta: {{precio_venta}}</label><label class="m-1">Categoria: {{categoria_id}}</label>
                <label class="m-1">Ingresado hace: {{timeago fecha_ingreso}}</label>
            </lu>
        {{/each}}

    </div>
</div>